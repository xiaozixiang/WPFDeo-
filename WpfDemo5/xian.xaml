<Window x:Class="WpfDemo5.xian"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="xian" Height="300" Width="300">
   
    <Grid x:Name="mianwindow">
        
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        
            <Canvas>
                <Rectangle Stroke="Red" Height="65" Width="95" Canvas.Left="142" Canvas.Top="47"></Rectangle>
                <Ellipse Canvas.Left="25" Canvas.Top="20" Width="90" Height="76" Fill="Red"></Ellipse>
                <Polygon Fill="DarkBlue" Points="10,80 45,69 30,45" ></Polygon>
                <Polyline Points="5,10 50,36 36,89" StrokeDashArray=""  Canvas.Top="77" Height="99" Width="142" Fill="Red"></Polyline>
            <Button Width="200" Height="200" Style="{Binding button1}"> asdfasdf</Button>

            </Canvas>
        <Viewbox Grid.Row="1">
            <StackPanel>
                <Rectangle Width="200" Height="200" Stroke="Blue"></Rectangle>
                <Ellipse Height="120" Width="120" Stroke="Yellow" Fill="Yellow"></Ellipse>
            </StackPanel>
           
           
        </Viewbox>
        
    </Grid>
   
</Window>
